20 // Initial speed is fast\nlet speedMultiplier = 1\n\nradio.setGroup(10)\n\nradio.onReceivedNumber(function (receivedNumber) {\n    speedMultiplier = Math.max(1, receivedNumber)\n    delayTime = Math.max(10, 30 - speedMultiplier) // Inverse logic, higher number = slower\n})\n\nbasic.forever(function () {\n    if (currentLed == numLeds - 1) {\n        startTime = input.runningTime()\n    }\n    if (currentLed >= 0) {\n        strip.setPixelColor(currentLed, getColorForLed(currentLed))\n        strip.show()\n        basic.pause(delayTime) // Adjusted speed based on the multiplier\n        currentLed--\n    } else {\n        roundTime = input.runningTime() - startTime\n        // Animation du chiffre qui arrive de la droite\n        for (let i = 0; i <= 4; i++) {\n            basic.showNumber(speedMultiplier)\n            basic.pause(200)\n            basic.clearScreen()\n            basic.pause(200)\n        }\n        basic.showNumber(speedMultiplier)\n        basic.clearScreen()\n        currentLed = numLeds - 1\n        strip.clear()\n    }\n})","README.md":"","pxt.json":"{\n    \"name\": \"test prof new méthode\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\",\n        \"neopixel\": \"github:microsoft/pxt-neopixel#v0.7.6\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}},{"timestamp":1740046434622,"editorVersion":"7.0.57","text":{"main.blocks":"<xml xmlns=\"https://developers.google.com/blockly/xml\"><variables><variable id=\"}uVwS%3W,TheAmtp%`w)\">ratio</variable><variable id=\"MhqZbBlWhxWZBq|YI~vH\">numLeds</variable><variable id=\"q[K6%UAPrsJd5|+OjjZl\">red</variable><variable id=\",L%{$mbr2r#Et(Gxc}kK\">green</variable><variable id=\"-{$_3:f?*sShG`!kcSnW\">delayTime</variable><variable id=\"(HW]P.?{pL#QSiJ~2gNo\">currentLed</variable><variable id=\"@G$irr),m,aPK`t)wfo;\"